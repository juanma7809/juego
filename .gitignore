<meta charset="utf-8" />
    <title>Gamedev Canvas Workshop</title>
    <style>
    	* { padding: 0; margin: 0; }
    	canvas { background: #ffe; display: block; margin: 0 auto; }
    </style>
</head>
<body>

<canvas id="myCanvas" width="880" height="620"></canvas>

<script>
	var canvas = document.getElementById("myCanvas");
	var ctx = canvas.getContext("2d");
	ctx.rect(50,50, 290, 290);
    ctx.fillStyle = "#FF0000";
    ctx.fill();
    ctx.closePath();
    ctx.arc(600, 360, 150, 0, Math.PI*2, false);
    ctx.fillStyle = "33FF61";
    ctx.fill();
    ctx.rect(450, 10, 200, 80);
     ctx.fillStyle = "4633FF";
    ctx.stroke();
    ctx.closePath();
</script>
</body>
</html>
